<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="your points:">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <Text fx:id="_points" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="64.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Question:">
               <font>
                  <Font size="24.0" />
               </font></Text>
            <Text fx:id="_question" strokeType="OUTSIDE" strokeWidth="0.0" text="ques">
               <font>
                  <Font size="18.0" />
               </font></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Options:">
               <font>
                  <Font size="24.0" />
               </font></Text>
            <ChoiceBox fx:id="_options" prefWidth="150.0" />
            <Button fx:id="_mainButton" mnemonicParsing="false" onAction="#update" prefHeight="39.0" prefWidth="150.0" text="submit">
               <font>
                  <Font size="18.0" />
               </font></Button>
            <Text fx:id="_systemOutput" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Button fx:id="_restart" mnemonicParsing="false" onAction="#restart" text="restart" />
         </children>
      </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
